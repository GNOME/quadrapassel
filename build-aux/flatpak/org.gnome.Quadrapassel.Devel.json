{
  "id": "org.gnome.Quadrapassel.Devel",
  "runtime": "org.gnome.Platform",
  "runtime-version": "master",
  "sdk": "org.gnome.Sdk",
  "command": "quadrapassel",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--socket=wayland",
    "--socket=pulseaudio",
    "--device=dri",
    "--device=all"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/share/pkgconfig",
    "/share/aclocal",
    "/man",
    "/share/man",
    "/share/gtk-doc",
    "*.la",
    "*.a",
    "/lib/girepository-1.0",
    "/share/dbus-1",
    "/share/doc",
    "/share/gir-1.0"
  ],
  "modules": [
    {
      "name": "libmanette",
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/GNOME/libmanette.git",
          "branch": "libmanette-0-2"
        }
      ],
      "modules": [
        {
          "name": "libevdev",
          "sources": [
            {
              "type": "git",
              "url": "https://gitlab.freedesktop.org/libevdev/libevdev.git"
            }
          ]
        }
      ]
    },
    {
      "name": "quadrapassel",
      "buildsystem": "meson",
      "config-opts": [
        "-Dprofile=development"
      ],
      "sources": [
        {
          "type": "git",
          "url": "https://gitlab.gnome.org/GNOME/quadrapassel.git"
        }
      ]
    }
  ]
}
